{
    "name": "c14r/laravel-data-store",
    "description": "A flexible polymorphic key-value storage system for Laravel with namespaces, TTL support, and nested data structures",
    "keywords": [
        "laravel",
        "data-store",
        "key-value",
        "storage",
        "polymorphic",
        "namespace",
        "ttl",
        "cache"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Pfeifer",
            "email": "mail@c14r.de"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "suggest": {
        "spatie/laravel-data": "For seamless Data Transfer Object integration with automatic conversion"
    },
    "autoload": {
        "psr-4": {
            "C14r\\DataStore\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "C14r\\DataStore\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "C14r\\DataStore\\DataStoreServiceProvider"
            ],
            "aliases": {
                "DataStore": "C14r\\DataStore\\Facades\\DataStore"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    }
}